Algoritmo G1_ejer1
	Definir pesos, dolar,euro, rial Como Real;
	Escribir "Ingrese cantidad de Pesos";
	Leer pesos;
	Escribir "Ingrese cotización del dólar, euro y real";
	Leer dolar,euro,rial;
	Escribir "$" ,pesos, " pesos equivalen a: " ,pesos/dolar, " dolares, a: ", pesos/euro, " euros y a " ,pesos/rial, " reales"; 
FinAlgoritmo
