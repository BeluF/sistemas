Algoritmo ejercicio3b
	Definir mm,dd,aa Como Cadena;
	Escribir "Ingrese fecha en formato MMDDAA";
	Escribir Sin Saltar "Mes ";
	Leer  mm;
	Escribir Sin Saltar "Dia ";
	Leer dd;
	Escribir Sin Saltar "Año ";
	Leer aa;
	Escribir "La fecha actual en formato DD/MM/AA es: ", dd,"/",mm,"/",aa;
FinAlgoritmo
