Algoritmo Algorithm
	Definir sueldo Como Entero;
	Escribir "Ingrese su sueldo:";
	Leer Sueldo;
	Escribir "Su aporte jubilatorio debe ser de ", 16*(sueldo/100), " y su sueldo líquido es ", sueldo-(16*(sueldo/100));
FinAlgoritmo

